<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <ioweb_eurobank>
                <debug>1</debug>
                <debug_logging>1</debug_logging>
                <debug_enabled>1</debug_enabled>
                <currency>EUR</currency>
                <model>EurobankFacade</model>
                <active>0</active>
                <title>Credit Card (Eurobank)</title>
                <payment_action>authorize</payment_action>
                <is_gateway>1</is_gateway>
                <can_order>1</can_order>
                <can_use_checkout>1</can_use_checkout>
                <can_use_internal>1</can_use_internal>
                <can_authorize>1</can_authorize>
                <can_capture>1</can_capture>
                <can_refund>0</can_refund>
                <can_void>0</can_void>
                <can_cancel>0</can_cancel>
                <!--<cctypes>AE,VI,MC,DI,JCB,CUP,DN,MI</cctypes>-->
                <!--<useccv>1</useccv>-->
                <order_status>processing</order_status>
                <privateInfoKeys>merchant_id,shared_secret</privateInfoKeys>
                <paymentInfoKeys>enable_installments,installments</paymentInfoKeys>
            </ioweb_eurobank>
        </payment>
    </default>
</config>